{
  "name": "netease-cloud-music",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "lint:css": "stylelint src/**/*.{css,less,scss,sass} --fix --custom-syntax postcss-less",
    "lint:vue": "stylelint src/**/*.{vue,htm,html} --fix --custom-syntax postcss-html",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -n ./.changelog.config.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vue/cli-plugin-eslint": "^5.0.7",
    "@vue/cli-plugin-router": "^5.0.7",
    "@vue/cli-plugin-typescript": "^5.0.7",
    "@vue/cli-plugin-vuex": "^5.0.7",
    "@vue/cli-service": "^5.0.7",
    "@vue/eslint-config-typescript": "^11.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-less": "^1.0.5",
    "stylelint-order": "^5.0.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
